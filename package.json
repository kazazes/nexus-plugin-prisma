{
  "name": "nexus-plugin-prisma",
  "version": "0.0.0-dripip",
  "license": "MIT",
  "author": "Jason Kuhrt",
  "main": "dist/index.js",
  "module": "dist/mylib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "global-type.d.ts"
  ],
  "scripts": {
    "dev": "tsdx watch",
    "build": "node build-scripts/update-hash.js && tsdx build",
    "test": "tsdx test",
    "test:e2e": "jest --testTimeout=240000 test/e2e",
    "lint": "tsdx lint",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "release:stable": "dripip stable",
    "release:preview": "dripip preview",
    "publish:pr": "PR=$(hub pr show -f '%I') && yarn publish --tag pr.${PR} --no-git-tag-version --new-version \"0.0.0-pr.${PR}.$(git show -s head --format='%h')\" && git checkout package.json",
    "prepack": "yarn -s build",
    "postinstall": "mkdir -p \"$PWD/../@types/nexus-plugin-prisma\" && cp \"$PWD/global-type.d.ts\" \"$PWD/../@types/nexus-plugin-prisma/index.d.ts\""
  },
  "peerDependencies": {
    "nexus-future": "^0.0.0-pr.419.17.135d958"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^25.1.3",
    "@types/prismjs": "^1.16.0",
    "dripip": "^0.6.1",
    "husky": "^4.2.1",
    "nexus-future": "^0.0.0-pr.419.17.135d958",
    "node-fetch": "^2.6.0",
    "node-pty": "^0.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "strip-ansi": "^6.0.0",
    "tsdx": "^0.12.0",
    "tslib": "^1.11.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview022",
    "@prisma/generator-helper": "^0.0.40",
    "@prisma/sdk": "^0.0.216",
    "@prisma/studio-server": "0.198.0",
    "chalk": "^3.0.0",
    "common-tags": "^1.8.0",
    "fs-jetpack": "^2.2.3",
    "get-port": "^5.1.1",
    "nexus": "^0.12.0-rc.13",
    "nexus-prisma": "^0.10.0",
    "open": "^7.0.0",
    "prisma2": "2.0.0-preview022",
    "prismjs": "^1.19.0",
    "stacktrace-parser": "^0.1.9"
  },
  "prisma": {
    "version": "f608db3b9e4f50f9a384356e532357b0a525969a"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
