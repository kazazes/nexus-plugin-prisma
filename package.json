{
  "name": "graphql-santa-plugin-prisma",
  "version": "0.1.2",
  "license": "MIT",
  "author": "Jason Kuhrt",
  "main": "dist/index.js",
  "module": "dist/mylib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "global-type.d.ts"
  ],
  "scripts": {
    "dev": "tsdx watch",
    "build": "node build-scripts/update-hash.js && tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "release:stable": "dripip stable",
    "release:preview": "dripip preview",
    "publish:pr": "PR=$(hub pr show -f '%I') && yarn publish --tag pr.${PR} --no-git-tag-version --new-version \"0.0.0-pr.${PR}.$(git show -s head --format='%h')\" && git checkout package.json",
    "prepack": "yarn -s build",
    "postinstall": "mkdir -p \"$PWD/../@types/graphql-santa-plugin-prisma\" && cp \"$PWD/global-type.d.ts\" \"$PWD/../@types/graphql-santa-plugin-prisma/index.d.ts\""
  },
  "peerDependencies": {
    "graphql-santa": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^24.9.1",
    "@types/prismjs": "^1.16.0",
    "dripip": "^0.5.0",
    "graphql-santa": "^0.3.0",
    "husky": "^4.2.1",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tsdx": "^0.12.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview020.3",
    "@prisma/generator-helper": "^0.0.38",
    "@prisma/sdk": "^0.0.172",
    "@prisma/studio-server": "^0.189.0",
    "chalk": "^3.0.0",
    "common-tags": "^1.8.0",
    "fs-jetpack": "^2.2.3",
    "get-port": "^5.1.1",
    "nexus-prisma": "^0.7.0",
    "open": "^7.0.0",
    "prisma2": "2.0.0-preview020.3",
    "prismjs": "^1.19.0",
    "stacktrace-parser": "^0.1.8"
  },
  "prisma": {
    "version": "45dd3493ff2fd09b5a8cdf697de276e368f7ebee"
  }
}
