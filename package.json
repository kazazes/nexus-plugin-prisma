{
  "name": "pumpkins-plugin-prisma",
  "version": "0.0.0-master.cd02e52",
  "license": "MIT",
  "author": "Jason Kuhrt",
  "main": "dist/index.js",
  "module": "dist/mylib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "publish:master": "[ \"$(git rev-parse --abbrev-ref HEAD)\" = \"master\" ] && yarn publish --tag master --no-git-tag-version --new-version \"0.0.0-master.$(git show -s head --format='%h')\"",
    "publish:pr": "PR=$(hub pr show -f '%I') && yarn publish --tag pr.${PR} --no-git-tag-version --new-version \"0.0.0-pr.${PR}.$(git show -s head --format='%h')\"",
    "prepack": "yarn -s build"
  },
  "peerDependencies": {
    "pumpkins": "master"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^24.0.23",
    "@types/prismjs": "^1.16.0",
    "husky": "^3.1.0",
    "pumpkins": "^0.0.0-master.b2a3f5e",
    "tsdx": "^0.11.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "common-tags": "^1.8.0",
    "fs-jetpack": "^2.2.3",
    "nexus-prisma": "^0.6.1",
    "prismjs": "^1.17.1"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ]
  }
}
