{
  "name": "graphql-santa-plugin-prisma",
  "version": "0.1.2",
  "license": "MIT",
  "author": "Jason Kuhrt",
  "main": "dist/index.js",
  "module": "dist/mylib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "global-type.d.ts"
  ],
  "scripts": {
    "dev": "tsdx watch",
    "build": "node build-scripts/update-hash.js && tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "publish:stable": "[ \"$(git rev-parse --abbrev-ref HEAD)\" = \"master\" ] && yarn publish --tag latest && git push && git push --tags",
    "publish:master": "[ \"$(git rev-parse --abbrev-ref HEAD)\" = \"master\" ] && yarn publish --tag master --no-git-tag-version --new-version \"0.0.0-master.$(git show -s head --format='%h')\" && git commit -am 'chore: version' && git push",
    "publish:pr": "PR=$(hub pr show -f '%I') && yarn publish --tag pr.${PR} --no-git-tag-version --new-version \"0.0.0-pr.${PR}.$(git show -s head --format='%h')\" && git checkout package.json",
    "prepack": "yarn -s build",
    "postinstall": "mkdir -p $(PWD)/node_modules/@types/graphql-santa-plugin-prisma && cp $(PWD)/node_modules/graphql-santa-plugin-prisma/global-type.d.ts $(PWD)/node_modules/@types/graphql-santa-plugin-prisma/index.d.ts"
  },
  "peerDependencies": {
    "graphql-santa": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^24.0.24",
    "@types/prismjs": "^1.16.0",
    "graphql-santa": "^0.2.0",
    "husky": "^3.1.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "tsdx": "^0.12.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@prisma/generator-helper": "^0.0.31",
    "@prisma/photon": "2.0.0-preview019",
    "@prisma/sdk": "^0.0.133",
    "@prisma/studio-server": "^0.179.0",
    "chalk": "^3.0.0",
    "common-tags": "^1.8.0",
    "fs-jetpack": "^2.2.3",
    "get-port": "^5.1.0",
    "nexus-prisma": "^0.6.2-next.3",
    "open": "^7.0.0",
    "prisma2": "2.0.0-preview019",
    "prismjs": "^1.17.1",
    "stacktrace-parser": "^0.1.8"
  },
  "prisma": {
    "version": "e7579bd35e0938dbf773f1706c098a0d14a5a038"
  }
}
